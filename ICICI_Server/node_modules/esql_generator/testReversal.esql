BROKER SCHEMA com.icici.xpress_connect.ecollection

CREATE COMPUTE MODULE ecollection_LEND_File_To_JSON
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		 CALL CopyEntireMessage();

	DECLARE ns1 NAMESPACE 'http://www.icici.com/xpress-connect/ecollection';
		
		-- Code --


SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."Client Code"=CAST(reversalFile.ClientCode AS INTEGER CCSID 1208);
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."Virtual Account Number"=reversalFile.VirtualAccountNumber;
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."Transaction Amount"=CAST(reversalFile.TransactionAmount AS DECIMAL(28,2));
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."Mode"=reversalFile.PaymentMode;
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."UTR number"=reversalFile.UTRnumber;
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."Sender Name"=reversalFile.SenderName;
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."Remitter Account Number"=reversalFile.RemitterAccountNumber;
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."Sender IFSC_Code"=reversalFile.SenderIFSC_Code;
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."Date"=reversalFile.Date;
SET OutputRoot.JSON.Data.Bank."Virtual Account Number Verification IN"."item"."sender to receiver information"=reversalFile.sendertoreceiverinformation;



		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
