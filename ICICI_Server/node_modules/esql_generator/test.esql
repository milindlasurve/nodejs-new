BROKER SCHEMA com.icici.xpress_connect.ecollection

CREATE COMPUTE MODULE ecollection_LEND_File_To_JSON
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		 CALL CopyEntireMessage();

	DECLARE ns1 NAMESPACE 'http://www.icici.com/xpress-connect/ecollection';
		
		-- Code --
CREATE FIELD OutputRoot.JSON.Data."Virtual Account Number Verification IN" IDENTITY(JSON.Array) "Virtual Account Number Verification IN";


SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."Client Code"=CAST(InputRoot.DFDL.ns1:REL_Input.record.ClientCode AS INTEGER CCSID 1208);
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."Virtual Account Number"=InputRoot.DFDL.ns1:REL_Input.record.VirtualAccountNumber;
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."Transaction Amount"=CAST(InputRoot.DFDL.ns1:REL_Input.record.TransactionAmount AS DECIMAL(28,2));
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."Mode"=InputRoot.DFDL.ns1:REL_Input.record.PaymentMode;
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."UTR number"=InputRoot.DFDL.ns1:REL_Input.record.UTRnumber;
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."Sender Name"=InputRoot.DFDL.ns1:REL_Input.record.SenderName;
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."Remitter Account Number"=InputRoot.DFDL.ns1:REL_Input.record.RemitterAccountNumber;
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."Sender IFSC_Code"=InputRoot.DFDL.ns1:REL_Input.record.SenderIFSC_Code;
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."Date"=InputRoot.DFDL.ns1:REL_Input.record.Date;
SET OutputRoot.JSON.Data."Virtual Account Number Verification IN"."item"."sender to receiver information"=InputRoot.DFDL.ns1:REL_Input.record.sendertoreceiverinformation;



		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
